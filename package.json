{
  "name": "genlogic",
  "version": "1.0.0",
  "description": "GenLogic - Augmented Normalization for PostgreSQL with foreign keys as data pipelines",
  "main": "dist/index.js",
  "bin": {
    "genlogic": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "node --loader tsx/esm src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:validation": "jest --testPathPattern=validation",
    "test:database": "jest --testPathPattern=database",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "database",
    "postgresql",
    "schema",
    "automation",
    "normalization"
  ],
  "author": "GenLogic Team",
  "license": "AGPL-3.0",
  "dependencies": {
    "ajv": "^8.12.0",
    "commander": "^11.1.0",
    "pg": "^8.11.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.4",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module"
}
